<% if @notify[:success] %>
	notify('success', '<%=  @notify[:message] %>');
	$('#modal-create-exam-structure').modal('hide');
	$('.list-exam-structures').html('<%= j render "admin/courses/list_exam_structures",\
  	course: @course %>');
<% else %>
	notify('error', '<%= @notify[:message] %>');
<% end %>